
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="google-adsense-account" content="ca-pub-8516076386707931">
<title>{% block title %} {{ title }} {% endblock %}</title>
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<link rel="stylesheet" href="/static/css/main.css"/>
<link rel="icon" type="image/png" sizes="96x96" href="/static/images/favicon-96x96.png">
<script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-8516076386707931"
crossorigin="anonymous">

{# <!-- Open Graph Meta Tags -->
<!-- Twitter Card Meta Tags --> #}
<meta property="og:title" content="BreezyParks{% if title %}: {{title}}{% endif%}" />
<meta name="twitter:title" content="BreezyParks{% if title %}: {{title}}{% endif%}" />

{% if meta_description is not defined %}
	<meta name="twitter:description" content="Breezyparks" />
	<meta property="og:description" content="Breezyparks" />
{% else %}
	<meta name="twitter:description" content="{{meta_description}}" />
	<meta property="og:description" content="{{meta_description}}" />
{% endif %}

{% if meta_image is not defined %}
	<meta property="og:image" content="https://breezyparks.com/static/images/favicon-96x96.png" />
	<meta name="twitter:image" content="https://breezyparks.com/static/images/favicon-96x96.png" />
{% else %}
	<meta property="og:image" content="{{meta_image}}" />
	<meta name="twitter:image" content="{{meta_image}}" /> 
{% endif%}

<meta property="og:url" content="https://breezyparks.com/" />
<meta property="og:type" content="website" />
<meta name="twitter:card" content="summary_large_image" />

</script>