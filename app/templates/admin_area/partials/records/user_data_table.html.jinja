		<td id="user-data-created_date-action"></td></tr>
		<tr><td id="user-data-last_login_date-label">last_login_date</td>
		<td id="user-data-last_login_date-data">{{user.last_login_date}}</td>
		<td id="user-data-last_login_date-action"></td></tr>
		<tr><td id="user-data-email-label">email</td>
		<td id="user-data-email-data">{{user.email}}</td>
		<td id="user-data-email-action"></td></tr>
		<tr><td id="user-data-google_id-label">google_id</td>
