
{% if user is defined and current_user.is_admin %}
<div id="data-accordian">
<table class="my-neat-table">
	<tr>
		<th>Name</th>
		<th>Value</th>
		<th>Action</th>
	</tr>
	<tbody>
		<tr><td id="user-data-id-label">id</td>
		<td id="user-data-id-data">{{user.id}}</td>
		<td id="user-data-id-action"></td></tr>
		<tr><td id="user-data-username-label">username</td>
		<td id="user-data-username-data">{{user.username}}</td>
		<td id="user-data-username-action"></td></tr>
