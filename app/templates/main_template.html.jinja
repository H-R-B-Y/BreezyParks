<!DOCTYPE html>
<html lang="en">
<head>
	{% block head %}
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>{% block title %} Title {% endblock %}</title>
		<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
		<link rel="stylesheet" href="{{ url_for('static', filename='main.css') }}"/>
		<script src="https://pixijs.download/release/pixi.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.7.4/socket.io.min.js"></script>
	{% endblock %}
	{% block extraHead %}{% endblock %}
</head>
	<body class="w3-text-white " style="background:#1f2122;">
		{% block background%}{% if prevent_zeta is not defined %}{% if current_user.is_authenticated %}
			<div class="background">
				<canvas id="zetaCanvas" data-userid="{{ current_user.username }}"></canvas>
				<script src="{{ url_for('static', filename='zetaSocketIO.js')}}"></script>
			</div>{% endif %}{% endif %}
		{% endblock %}
		{% block foreground %}
			<div class="foreground">
				{% include "partials/flash.html.jinja" %}
				{% block header %} {% include "partials/header.html.jinja" %} {% endblock %}
				<div class="bodyContent w3-container w3-card">
				{% block body %}
						<div id="main">
							<p>Hello World</p>
						</div>
				{% endblock %}
				</div>
				{% block footer %} {% include "partials/footer.html.jinja" %}  {% endblock %}
			</div> 
		{% endblock %}
	</body>
</html>